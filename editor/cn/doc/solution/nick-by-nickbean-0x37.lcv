```java
class Solution {
    public List<Integer> findDisappearedNumbers(int[] nums) {
        List<Integer> ans = new ArrayList<>();
        int len = nums.length;
        for (int num : nums){
            int index = (num - 1) % len;
            nums[index] += len;
        }
        for (int i = 0;i < len;i++){
            if(nums[i] <= len){
                ans.add(i + 1);
            }
        }
        return ans;
    }
}
